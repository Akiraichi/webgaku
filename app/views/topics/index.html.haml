                    %p Lorem ipsum dolor sit amet consectetur adipisicing
                .wow.fadeInUp{"data-wow-delay" => "0.9s", "data-wow-offset" => "0"}
                  %form.form-inline
                    .form-group
                      %button.btn.btn-primary.btn-lg{:name => "Live Demo", :required => "required", :type => "livedemo"} Live Demo
                    .form-group
                      %button.btn.btn-primary.btn-lg{:name => "Get Now", :required => "required", :type => "getnow"} Get Now
            .item
              %img.img-responsive{:alt => "", :src => "/img/takkyu.jpg"}/
              .carousel-caption
                .wow.fadeInUp{"data-wow-delay" => "1.0s", "data-wow-offset" => "0"}
