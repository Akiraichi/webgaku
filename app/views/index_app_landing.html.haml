    / ========== HEADER ==========
    %header.navbar-fixed-top.s-header.js__header-sticky.js__header-overlay
      / Navbar
      .s-header__navbar
        .s-header__container
          .s-header__navbar-row
            .s-header__navbar-row-col
              / Logo
              .s-header__logo
                %a.s-header__logo-link{:href => "index.html"}
                  %img.s-header__logo-img.s-header__logo-img-default{:alt => "Megakit Logo", :src => "img/logo-white.png"}/
